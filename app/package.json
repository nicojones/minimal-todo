{
  "name": "todo",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://minimaltodo.com",
  "scripts": {
    "start-local": "react-scripts start --host=0.0.0.0",
    "start": "react-scripts start",
    "build-sm": "react-scripts build --production",
    "build": "GENERATE_SOURCEMAP=false react-scripts build --production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analise": "source-map-explorer 'build/static/js/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "jotai": "^1.12.1",
    "jwt-decode": "3.1.2",
    "materialize-css": "^1.0.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "5.3.4",
    "rxjs": "^7.8.0",
    "sha1": "^1.1.1",
    "timeago.js": "^4.0.2",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^18.0.26",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.8.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/parser": "^5.46.1",
    "gh-pages": "^4.0.0",
    "node-sass": "^7.0.3",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
