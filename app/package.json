{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nicojones.github.io/todolist-react",
  "scripts": {
    "start": "react-scripts start",
    "build-sm": "react-scripts build --production",
    "build": "GENERATE_SOURCEMAP=false react-scripts build --production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analise": "source-map-explorer 'build/static/js/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cogo-toast": "^4.2.3",
    "firebase": "^8.0.2",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^16.9.56",
    "@typescript-eslint/parser": "^4.8.2",
    "gh-pages": "^3.1.0",
    "materialize-css": "^1.0.0-rc.2",
    "node-sass": "^4.0.0",
    "source-map-explorer": "^2.5.1",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
